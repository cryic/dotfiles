	"IEx Pry in console": {
		"prefix": "pry",
		"body": [
			"require IEx",
			"IEx.pry()"
		],
		"description": "Insert the require and pry statement"
	},
	"Inspect value": {
		"prefix": "inspect",
		"body": [
			"IO.inspect(${1:variable}, label: \"${1:variable}\")"
		],
		"description": "Insert IO.inspect with label"
	}
}
